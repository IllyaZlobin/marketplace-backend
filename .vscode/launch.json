  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Nest Framework",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "start:debug", "--", "--inspect-brk"],
      "sourceMaps": true,
      "console": "integratedTerminal",
      "restart": true,
      "autoAttachChildProcesses": true,
      "env": {
        "APP_ENV": "local"
      }
    },
  ]
}
